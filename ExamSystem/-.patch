+        //页码越界
+        //获取数据的总个数
+        int count = classMapper.countClass(classVO);
+        //计算最大页数
+        int max = (count + classVO.getPageSize() - 1) / classVO.getPageSize();
+        if (classVO.getPageCode() > max) {
